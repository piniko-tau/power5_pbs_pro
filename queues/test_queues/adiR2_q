#
# Create and define queue adisR2
#
create queue adisR2
set queue adisR2 queue_type = Route
set queue adisR2 acl_user_enable = True
set queue adisR2 acl_users = piniko
set queue adisR2 acl_users += dvory
set queue adisR2 resources_max.walltime = 2400:00:00
set queue adisR2 route_destinations += AdisE
set queue adisR2 from_route_only = True
set queue adisR2 enabled = True
set queue adisR2 started = True
